<% @res['data'].each do |video| %>
  <%= link_to video['attributes']['url'], target: '_blank' do%>
    <div class="col-sm-6 col-md-3">
      <div class="thumbnail" style="height: 220px">
        <%= image_tag(video['attributes']['preview-image-url'], size: '320x180')%>
        <br>
        <p style="font-size: 12px; text-align: center">
          <%= truncate(video['attributes']['name'], length:35)  %>
        </p>
      </div>
    </div>
  <% end %>
<% end %>